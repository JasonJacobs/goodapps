<p id="notice"><%= notice %></p>

<p>
  <strong>Email:</strong>
  <%= @user.email %>
</p>

<p>
  <strong>Name:</strong>
  <%= @user.name %>
</p>

<p>
  <strong>Profile image:</strong>
  <%= @user.profile_image %>
</p>

<p>
  <strong>Lists:</strong>
  <% @lists.each do |list| %>
    <%= list.title if list.user_id == @user.id %>
  <% end %>
</p>

<%= link_to 'Edit', edit_user_path(@user) %> |
<%= link_to 'Back', users_path %>
